<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from CiaoMondoService WSDL service -->

  <service name="CiaoMondo" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://DefaultNamespace"/>
      <parameter name="wsdlServiceElement" value="CiaoMondoService"/>
      <parameter name="schemaQualified" value="http://DefaultNamespace"/>
      <parameter name="wsdlServicePort" value="CiaoMondo"/>
      <parameter name="className" value="DefaultNamespace.CiaoMondoSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="CiaoMondo"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getSperanza" qname="operNS:getSperanza" xmlns:operNS="http://DefaultNamespace" returnQName="retNS:getSperanzaReturn" xmlns:retNS="http://DefaultNamespace" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <parameter name="allowedMethods" value="getSperanza"/>

  </service>
</deployment>
